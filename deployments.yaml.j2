apiVersion: apps/v1
kind: Deployment
metadata:
  name: {{APP_NAME}}
  labels:
    app: {{APP_NAME}}
spec:
  replicas: 1
  selector:
    matchLabels:
      app: {{APP_NAME}}
  template:
    metadata:
      labels:
        app: {{APP_NAME}}
    spec:
      containers:
      - name: {{APP_NAME}}
        image: registry-1.docker.io/fbnquestdocker/{{IMAGE_NAME}}:{{IMG_TAG}}
        ports:
        - containerPort: 80
      imagePullSecrets:
        - name: fbnquest-registry-secret
